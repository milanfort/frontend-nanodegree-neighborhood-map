{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "source": {
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "includeDate": false,
    "useLongnameInNav": false
  },
  "opts": {
    "encoding": "utf8",
    "access": "all",
    "package": "./package.json",
    "readme": "./README.md",
    "destination": "./docs"
  }
}
